{
  "name": "@autositefix/cli",
  "version": "0.1.0",
  "description": "Command line interface for AutoSiteFix.",
  "bin": {
    "autositefix": "dist/index.js"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@autositefix/auditor": "workspace:*",
    "@autositefix/fixer": "workspace:*",
    "@autositefix/git": "workspace:*",
    "@autositefix/report": "workspace:*",
    "yargs": "^17.7.2"
  }
}
